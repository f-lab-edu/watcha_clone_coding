{
  "compilerOptions": {
    "target": "es5", // 변환할 JavaScript 버전
    "module": "commonjs", // 모듈 시스템 (e.g. commonjs, esnext)
    "outDir": "./dist", // 컴파일된 파일 출력 폴더
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": ["src/**/*"],
  "exclude": []
}
